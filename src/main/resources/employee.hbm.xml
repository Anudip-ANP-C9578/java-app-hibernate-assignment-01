<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.anudip.javaapp.entity.Employee" table="employee" dynamic-update="true">
		<id name="employee_Id" column="employeeId">
			<generator class="assigned"></generator>
		</id>
		<property name="first_Name" column="firstName"></property>
		<property name="last_Name" column="lastName"></property>
		<property name="dateofjoin" column="dateOfJoining"></property>
	</class>
</hibernate-mapping>