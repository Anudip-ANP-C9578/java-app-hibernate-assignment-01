<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.anudip.javaapp.entity.Department" table="department" dynamic-update="true">
		<id name="departmentId" column="department_Id">
			<generator class="assigned"></generator>
		</id>
		<property name="deptName" column="dept_name"></property>
		<property name="deptHead" column="head"></property>
	</class>
</hibernate-mapping>