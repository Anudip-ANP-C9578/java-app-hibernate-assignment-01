<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.anudip.javaapp.entity.Department" table="department">
		<id name="departmentId" column="department_id">
			<generator class="increment"></generator>
		</id>
		<property name="name" column="department_name"></property>
		<property name="head" column="department_head"></property>
	</class>
</hibernate-mapping>