<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.anudip.java.entity.Department" table="department_information">
        <id name="departmentId" column="depart_Id">
            <generator class="assigned" />
        </id>
        <property name="name" column="depart_Name" />
        <property name="head" column="depart_Head" />
        
    </class>
</hibernate-mapping>