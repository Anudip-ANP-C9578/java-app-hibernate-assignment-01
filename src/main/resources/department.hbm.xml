<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.anudip.javaapp.entity.Department" table="department">
		<id name="department_Id" column="departmentId">
			<generator class="assigned"></generator>
		</id>
		<property name="dept_Name" column="deptName"></property>
		<property name="deptHead" column="head"></property>
	</class>
</hibernate-mapping>