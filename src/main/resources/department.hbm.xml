<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.anudip.javaapp.entity.Department" table="department_information">
		<id name="departmentId" column="departmentId">
			<generator class="increment"></generator>
		</id>
		<property name="departmentName" column="departmentName"></property>
		<property name="head" column="head"></property>
	</class>
</hibernate-mapping>